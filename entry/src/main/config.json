{
  "app": {
    "bundleName": "com.hmosdemos.lite_wearable.how_to_make_music_controller_for_lite_wearable",
    "vendor": "hmosdemos",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {},
  "module": {
    "deviceType": [
      "liteWearable"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry"
    },
    "abilities": [
      {
        "name": ".MainAbility",
        "srcLanguage": "js",
        "srcPath": "MainAbility",
        "icon": "$media:icon",
        "description": "$string:MainAbility_desc",
        "label": "$string:MainAbility_label",
        "type": "page"
      }
    ],
    "metaData": {
      "customizeData": [
        {
          "name": "supportLists",
          "value": "com.your.phoneAppPackageName:phone_fingerprint",
          "extra": ""
        }
      ]
    },
    "js": [
      {
        "pages": [
          "pages/index/index",
          "pages/player/player",
          "pages/playlist/playlist",
          "pages/phoneNotConnected/phoneNotConnected"
        ],
        "name": ".MainAbility"
      }
    ]
  }
}